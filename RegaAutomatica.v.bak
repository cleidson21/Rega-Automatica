// Declaracao do modulo
module RegaAutomatica (H, M, L, Us, Ua, T, Sd, Ve, Al,
							  Bs, Vs, M7);	
							  
	// Declaracao de portas
	input H, M, L, Us, Ua, T, Sd;
	output Bs, Vs, Ve, Al, M7;
	
	// Declaracao dos fios intermediarios
	wire wire_crit, wire_bai, wire_med, wire_alt, wire_err;
	
	// Funcionamento do circuito
	
	// Chamando o modulo de Nivel e Erro com 
	NivelErro(H, M, L, Ve, Al, wire_err, wire_crit, wire_bai, wire_med, wire_alt);
	
endmodule 